    "def validateAcc(acc):\n",
    "    if len(str(acc)) != 11:\n",
    "        return (float('NaN'))\n",
    "    else:\n",
    "        if acc[9] != \"-\":\n",
    "            return (float('NaN'))\n",
    "        else:\n",
    "            return acc\n",
    "\n",
    "cleanPatients[\"Patient Acct #\"] = cleanPatients[\"Patient Acct #\"].apply(validateAcc)\n"
   ]
